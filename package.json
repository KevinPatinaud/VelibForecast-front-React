{
  "name": "velibforecast",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^16.18.3",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "axios": "^1.2.0",
    "google-map-react": "^2.2.0",
    "jest": "^27.5.1",
    "jest-dom": "^4.0.0",
    "react": "^18.2.0",
    "react-circle-flags": "^0.0.18",
    "react-dom": "^18.2.0",
    "react-facebook-login": "^4.1.1",
    "react-google-recaptcha": "^2.1.0",
    "react-intl": "^6.2.4",
    "react-router-dom": "^6.4.4",
    "react-scripts": "5.0.1",
    "reactstrap": "^9.1.5",
    "reaptcha": "^1.12.1",
    "typescript": "^4.9.3",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "test:ci": "react-scripts test \"--coverage\" \"--watchAll=false\" --detectOpenHandles"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@types/axios": "^0.14.0",
    "@types/google-map-react": "^2.1.7",
    "@types/jest": "^29.2.4",
    "@types/react-facebook-login": "^4.1.5",
    "@types/react-google-recaptcha": "^2.1.5"
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "collectCoverageFrom": [
      "./src/**",
      "!src/App.tsx",
      "!src/index.tsx",
      "!src/reportWebVitals.ts",
      "!src/*d.ts",
      "!src/services/Websocket/Websocket.service.ts"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 80,
        "branches": 60,
        "lines": 80,
        "functions": 80
      }
    },
    "coveragePathIgnorePatterns": [
      ".*__snapshots__/.*"
    ]
  }
}
